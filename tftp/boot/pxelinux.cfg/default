LABEL memtest
	MENU LABEL Memtest86+ v5.01
	KERNEL iso/memtest

LABEL hdt
	MENU LABEL HDT (Hardware Detection Tool)
	COM32 hdt.c32

LABEL plop
	MENU LABEL PLoP Boot Manager
	TEXT HELP
	Launch PLoP Boot Manager, to boot from USB and enable USB 2.0 on
	some BIOSes that only support on 1.1, if supported by the controller.
	ENDTEXT
	LINUX iso/plpbt.bin

LABEL lubuntu_test
	MENU LABEL Lubuntu (test)
    KERNEL iso/lubuntu/install/vmlinuz
    APPEND initrd=iso/lubuntu/install/initrd.gz root=iso/lubuntu.iso rootfstype=iso9660 rootflags=loop

